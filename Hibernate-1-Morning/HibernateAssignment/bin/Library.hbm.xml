<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>

	<class name ="com.spring.au.hibernate.Library" table ="LIBRARY">
	<id name="id" type="int" column ="id">
	<generator class="native"/>
	</id>
	<property name="BookName" column="BookName" type="string"/>
	<property name="AuthorName" column="AuthorName" type="string"/>
	<property name="PublishedYear" column="PublishedYear" type="int"/>
	<property name="price" column="price" type="int"/>
	
	
	
	</class>
</hibernate-mapping>